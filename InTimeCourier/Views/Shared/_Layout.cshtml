<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - In Time Logistic</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Prime Logistics", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @if (Session["Name"] != null && Session["UserId"] != null)
                    {
                       
                        
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                Master
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Party Master", "PartyList", "Courier")</li>
                                <li>@Html.ActionLink("Courier Mode", "AddCourierMode", "Courier")</li>
                                <li>@Html.ActionLink("Network Mode", "Index", "Network")</li>
                                <li>@Html.ActionLink("Rate Mapping", "MapRateSourceAndParty", "Source")</li>

                            </ul>
                        </li>
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                Courier
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Add Courrier", "AddCourrier", "CourierDetails")</li>
                                <li>@Html.ActionLink("Bill Details", "Index", "BillDetails")</li>
                                <li>@Html.ActionLink("Reports", "CourrierList", "CourierDetails")</li>

                            </ul>
                        </li>
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                Ledger
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    @Html.ActionLink("Bill Statement", "Index", "BillStatement")
                                </li>
                                <li>@Html.ActionLink("Rate List", "GetRateMappingList", "Source")</li>
                                @*<li><a href="#">Page 1-3</a></li>*@
                            </ul>
                        </li>
                        }
                        <li>@Html.ActionLink("Contact Us", "Contact", "Home")</li>
                    </ul>
                @if (Session["Name"] != null && Session["UserId"] != null)
                {
                    @Html.Partial("_LoginPartial")
                }
            </div>

        </div>
    </div>
    <div class="body-content">
        @RenderBody()
        <hr />
    </div>
    <footer class="navbar navbar-default navbar-fixed-bottom">
        <div class="container body-content">
            <a class="navbar-brand">&copy; @DateTime.Now.Year - Designed & Developed By- : JD Softech (+919029451159)</a>
        </div>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("scripts", required: false)

    <div class="modal fade" id="modalBillDetailsForm" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <h4 class="modal-title"><label id="lblHeader"> Bill Details</label></h4>
                </div>
                <div class="panel-body" id="divBillDetails">

                </div>
                <div class="modal-footer">

                    <button type="button" class="btn btn-deep-orange" data-dismiss="modal" aria-label="Close">
                        Close
                    </button>
                </div>
            </div>
        </div>
    </div>


    @*<div class="modal fade bd-example-modal-lg" id="modalBillDetailsForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content ">
                    <div class="modal-header text-center">
                        <h4 class="modal-title"><label id="lblHeader"> Bill Details</label></h4>
                    </div>
                    <div class="panel-body" id="divBillDetails">

                    </div>
                    <div class="modal-footer">

                        <button type="button" class="btn btn-deep-orange" data-dismiss="modal" aria-label="Close">
                            Close
                        </button>
                    </div>
                </div>
            </div>
        </div>*@
    <script>
        $(function () {
            $("#addtxtPeriodFrom").datepicker({
                changeMonth: true,
                changeYear: true
            });

            $("#addtxtPeriodFrom").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
    </script>
</body>
</html>
