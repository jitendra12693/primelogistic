@model dynamic
@{
    Layout = null;
    ViewBag.Title = "Courrier List";
    var loggedInUser = (InTimeCourier.Models.AdminUser)Session["LoggedInUser"];
}

<style tyle="text/css">
    @@media screen {
        p.bodyText {
            font-family: verdana, arial, sans-serif;
        }
    }

    @@media print {
        p.bodyText {
            font-family: georgia, times, serif;
        }
    }

    @@media screen, print {
        p.bodyText {
            font-size: 10pt;
        }
    }
</style>
<div id="SearchCourierlist">
    @if (Model != null)
    {
        <div>
            <table class="table table-bordered" id="myTable" style="width: 100%; font-size: 13px;">
                <tr>
                    <th style="text-align:center">
                        Sr.No.
                    </th>
                    <th style="text-align:center">
                        Book Date
                    </th>
                    <th style="text-align:center">
                        AWBNo.
                    </th>
                    <th style="text-align:center">
                        Mode
                    </th>
                    <th style="text-align:center">
                        Destination
                    </th>
                    <th style="text-align:center">
                        Weight
                    </th>
                    <th style="text-align:center">
                        ODA
                    </th>
                    <th style="text-align:center">
                        Amount
                    </th>
                    <th id="tdAction">Action</th>
                </tr>
                @if (Model != null)
                {
                    var cnt = 1;
                    foreach (InTimeCourier.Models.CorrierDetails item in Model.CorrierDetails1)
                    {
                        <tr>
                            <td style="text-align:center">
                                @cnt
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.BookingDate)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.CNNo)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.CourierMode)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.Location)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.Weight)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.ODACharges)
                            </td>
                            <td style="text-align:center">
                                @Html.DisplayFor(modelItem => item.Amount)
                            </td>
                            <td id="tdActionVal">
                                <a href="#" id="projectImages" onclick="SelectedCourrier(@item.CourrierId)" data-toggle="modal" data-target="#divUpdateCourrier">Edit</a>
                            </td>
                        </tr>
                        cnt++;
                    }
                }
                else
                {
                    <tr>
                        <td colspan="9">No Record found</td>
                    </tr>
                }
            </table>
        </div>
    }
